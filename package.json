  "devDependencies": {
    "@delucis/reading-data": "^0.8.2",
    "chai": "^4.2.0",
    "coveralls": "^3.0.4",
    "jsdoc": "^3.6.2",
    "minami": "^1.2.3",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3"
